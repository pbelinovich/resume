{
  "name": "resume",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "start": "cross-env NODE_ENV=development webpack-dev-server --progress",
    "build:web": "cross-env webpack --progress --config webpack.config-web.js && cross-env webpack --progress --config webpack.config-web-worker.js",
    "lint": "eslint ./src",
    "prettier": "prettier --config ./.prettierrc.js --write \"./src/**/*.{ts,tsx}\" \"!./src/**/*.d.ts\"",
    "build": "cross-env NODE_ENV=PRODUCTION META_MODE=none webpack --progress --config=webpack.config.js",
    "build:full": "npm run build && npm run generate:pdf",
    "generate:pdf": "node scripts/generate-pdf.js",
    "generate:pdf:ru": "node scripts/generate-pdf.js --ru-only",
    "generate:pdf:en": "node scripts/generate-pdf.js --en-only",
    "update:pdf": "npm run build:full && git add src/static-resources/resume*.pdf && git commit -m \"ðŸ“„ Update PDF resume\"",
    "prepublishOnly": "cross-env META_MODE=only_dts npm run build",
    "debug": "cross-env NODE_ENV=DEVELOPMENT DEBUG=true META_MODE=none webpack --progress --config=webpack.config-web.js --watch"
  },
  "dependencies": {
    "@chakra-ui/layout": "^2.3.1",
    "@chakra-ui/react": "^3.25.0",
    "react-router": "6.28.0",
    "react-router-dom": "6.28.0"
  },
  "devDependencies": {
    "@babel/core": "7.21.3",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-decorators": "7.21.0",
    "@babel/plugin-transform-modules-commonjs": "7.21.2",
    "@babel/plugin-transform-runtime": "7.21.0",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-typescript": "7.21.0",
    "@babel/runtime": "^7.28.3",
    "@svgr/core": "7.0.0",
    "@svgr/webpack": "7.0.0",
    "@teamsupercell/typings-for-css-modules-loader": "2.5.2",
    "@types/jest": "29.5.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "5.57.0",
    "@typescript-eslint/parser": "5.57.0",
    "autoprefixer": "10.4.14",
    "babel-loader": "9.1.2",
    "circular-dependency-plugin": "5.2.2",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "11.0.0",
    "cross-env": "7.0.3",
    "css": "3.0.0",
    "css-loader": "6.7.3",
    "eslint": "8.37.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "fork-ts-checker-webpack-plugin": "8.0.0",
    "html-webpack-plugin": "5.5.3",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "mini-css-extract-plugin": "2.7.5",
    "node-sass": "8.0.0",
    "postcss-loader": "7.1.0",
    "prettier": "2.8.7",
    "puppeteer": "^24.22.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "resolve-url-loader": "5.0.0",
    "sass-loader": "13.2.2",
    "style-loader": "^4.0.0",
    "ts-jest": "29.1.0",
    "ts-loader": "9.4.2",
    "typescript": "5.0.2",
    "webpack": "^5.101.3",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "author": "pbelinovich",
  "license": "MIT",
  "jest": {
    "testEnvironment": "jsdom",
    "testEnvironmentOptions": {
      "url": "http://localhost/"
    },
    "setupFiles": [
      "<rootDir>/test-setup.ts"
    ],
    "transformIgnorePatterns": [
      "^.+\\.js$"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.(ts|tsx|js)$": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/*.(ts|tsx)"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/src/dev"
    ],
    "collectCoverage": false,
    "coverageReporters": [
      "text",
      "text-summary",
      "html"
    ],
    "testPathIgnorePatterns": [],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      },
      "./src/**/": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    }
  }
}
